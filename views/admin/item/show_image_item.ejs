<!-- DataTables item -->
<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="home-tab">
    <div class="row">
        <% for(let i = 0; i < item.imageId.length; i++){ %>
            <div class="col-md-4">
                <div class="card mb-4">
                    <div class="card-body">
                        <img src="https://admin-hotelku.herokuapp.com/<%= item.imageId[i].imageUrl %>" alt="item" style="width: 100%; display: block;">
                    </div>
                </div>
            </div>
        <% } %>
    </div>
    <a href="/admin/item" class="btn btn-warning">Back</a>
</div>